<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CircleImageView">
        <item name="cornerSize">50%</item>
    </style>

    <style name="CircleImageViewCorner16">
        <item name="cornerSize">@dimen/dimen_corner_16</item>
    </style>

    <style name="CircleImageViewCorner8">
        <item name="cornerSize">@dimen/dimen_corner_8</item>
    </style>

    <style name="allCircleImageView">
        <item name="cornerSize">@dimen/dimen_corner_8</item>
    </style>

    <style name="leftCircleImageView">
        <item name="cornerSizeTopLeft">@dimen/dimen_corner_8</item>
        <item name="cornerSizeBottomLeft">@dimen/dimen_corner_8</item>
    </style>

    <style name="rightCircleImageView">
        <item name="cornerSizeTopRight">@dimen/dimen_corner_8</item>
        <item name="cornerSizeBottomRight">@dimen/dimen_corner_8</item>
    </style>

    <style name="topLeftCircleImageView">
        <item name="cornerSizeTopLeft">@dimen/dimen_corner_8</item>
    </style>

    <style name="botLeftCircleImageView">
        <item name="cornerSizeBottomLeft">@dimen/dimen_corner_8</item>
    </style>

    <style name="PrimaryButton">
        <item name="fontFamily">@font/roboto_medium</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_20</item>
        <item name="android:background">@drawable/shape_rectangle_orange_bg_corner_8</item>
        <item name="android:minHeight">@dimen/dimen_48</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dimen_text_size_14</item>
    </style>

</resources>
